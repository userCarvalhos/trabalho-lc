int main()
{
    
   int nulos,brancos,validos,eleitores,rb,rn,rv;

   printf("escreva o numero de eleitores");
   scanf("%d",&eleitores);
 
   printf("escreva a quantidade de votos BRANCOS");
   scanf("%d",&brancos);

   printf("escreva a quantidade de votos NULOS");
   scanf("%d",&nulos);

   printf("escreva a quantidade de votos VALIDOS");
   scanf("%d",&validos);

   rb=(brancos*100)/eleitores;
   rn=(nulos*100)/eleitores;
   rv=(validos*100)/eleitores;

   printf("\n\tA porcentagem de votos brancos e %d",rb);
   printf("\n\tA porcentagem de votos nulis e %d",rn);
   printf("\n\tA porcentagem de votos validos e %d",rv);
 
   return 0;  
    }
