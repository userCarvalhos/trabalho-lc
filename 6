int main()
{
    int base,altura,area;
    
    printf("qual a area da base do retangulo?");
    scanf("%d",&base);
    
    printf("qual a area da altura do retangulo?");
    scanf("%d",&altura);
    
    area=base * altura;
    
    printf("o valor da area do retangulo Ã© %d",area);
    return 0;
}
